<h1>Subscriptions #<%= @subscription.guid %></h1>
<table class="table table-hover table-striped">
  <tr>
    <th>Attribute</th>
    <th>Value</th>
  </tr>
  <% @subscription.attributes.sort.each do |key,value| %>
    <tr>
      <td><%= key %></td>
      <td><%= value %></td>
    </tr>
  <% end %>
</table>

<%= link_to "Back", subscriptions_path, :class => 'btn btn-primary' %>
